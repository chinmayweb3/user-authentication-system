<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dashboard</title>
  </head>
  <style>
    main {
      display: flex;
      height: 100vh;
      justify-content: center;
      align-items: center;
      flex-direction: column;
    }
  </style>
  <script>
    var baseUrl = "<%= process.env.baseUrl %>";
  </script>
  <script
    src="https://code.jquery.com/jquery-3.7.1.slim.js"
    integrity="sha256-UgvvN8vBkgO0luPSUl2s8TIlOSYRoGFAX4jlCIm9Adc="
    crossorigin="anonymous"
  ></script>
  <body>
    <main>
      <h4>Username: <span id="givenUsername"></span></h4>
      <div id="div-add"></div>
      <button
        onclick="(() => {
          console.log('adding');
const c =` 
        <form onsubmit='sendFormData(event)'>
          <label for='name'>Project Name:</label>
          <input type='text' id='name' name='name' required />
          <label for='web'>Website Name:</label>
          <input type='text' id='web' name='web' required />
          <button id='submit' data-loading='0' type='submit'>Submit</button>
      </form>
`;
  document.getElementById('div-add').insertAdjacentHTML('afterend',c);

      })()"
      >
        Add project
      </button>
    </main>
    <script defer src="/dashboard.js"></script>
  </body>
</html>
